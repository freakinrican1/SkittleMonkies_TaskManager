<h1>New Task</h1>


<%= form_for @task do |f| %>

	<%= f.label :task_name %>
	<%= f.text_field :task_name %>

	<%= f.label :status %>
	<%= f.text_field :status %>

  <%= f.label :email %>
  <%= f.text_field :email %>

	<%= f.label :description %>
	<%= f.text_field :description %>

	<%= f.submit %>

<% end %>

